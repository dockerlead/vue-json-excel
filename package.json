{
  "name": "@dockerlead/vue-json-excel",
  "version": "1.0.0",
  "description": "Download your JSON as an excel or CSV file directly from the browser",
  "main": "JsonExcel.vue",
  "scripts": {
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dockerlead/vue-json-excel.git"
  },
  "keywords": [
    "vue",
    "xls",
    "csv",
    "json",
    "export"
  ],
  "author": "Jose Javier Espinoza, Ihsan Oeddin, DockerLead",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dockerlead/vue-json-excel/issues"
  },
  "homepage": "https://github.com/dockerlead/vue-json-excel#readme",
  "dependencies": {
    "downloadjs": "^1.4.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,md,html}": [
      "prettier --write"
    ]
  }
}
